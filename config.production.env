# Production Environment Configuration
# Use this for production deployment

# Debug Settings - DISABLED for production
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOGS=false
VITE_ENABLE_PERFORMANCE_LOGS=false

# Supabase Configuration (replace with your actual production values)
VITE_SUPABASE_URL=your_production_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key_here

# Application Settings
VITE_APP_NAME=PrepPro
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Performance Settings
VITE_QUESTION_BATCH_SIZE=100
VITE_MAX_QUESTIONS_PER_TEST=1000
VITE_ENABLE_QUESTION_CACHING=true

# Feature Flags
VITE_ENABLE_SMART_ROTATION=true
VITE_ENABLE_QUESTION_TRACKING=true
VITE_ENABLE_CSV_UPLOAD=true
